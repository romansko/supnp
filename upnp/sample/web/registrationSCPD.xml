<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">

  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>


  <actionList>

    <action>
      <name>Register</name>
      <argumentList>
         <argument>
           <name>SpecificationDocument</name>
           <relatedStateVariable>SpecDoc</relatedStateVariable>
           <direction>in</direction>
          </argument>
        <argument>
          <name>CertificateDevice</name>
          <relatedStateVariable>CertDev</relatedStateVariable>
          <direction>in</direction>
        </argument>
        <argument>
          <name>CertificateUCA</name>
          <relatedStateVariable>CertUCA</relatedStateVariable>
          <direction>in</direction>
        </argument>
        <argument>
          <name>DescriptionDocumentURL</name>
          <relatedStateVariable>DescDocUrl</relatedStateVariable>
          <direction>in</direction>
        </argument>
      </argumentList>
    </action>

    <action>
      <name>Challenge</name>
      <argumentList>
        <argument>
          <name>ChallengeResponse</name>
          <relatedStateVariable>ChResp</relatedStateVariable>
          <direction>in</direction>
        </argument>
        <argument>
          <name>PublicKey</name>
          <relatedStateVariable>PubKey</relatedStateVariable>
          <direction>in</direction>
        </argument>
      </argumentList>
    </action>

  </actionList>

  <serviceStateTable>
  </serviceStateTable>

</scpd>
















